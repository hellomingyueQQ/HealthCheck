{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "Blazor Demo",
        "Uri": "https://localhost:7017/health"
      },
      {
        "Name": "MVC Demo",
        "Uri": "https://localhost:7178/health"
      }
    ],
    "EvaluationTimeInSeconds": 5,
    "MinimumSecondsBetweenFailureNotification": 60
  }
}
